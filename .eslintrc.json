{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-console": 0,
    "no-mixed-operators": 0,
    "no-use-before-define": 0,
    "semi": 0,

    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",

    "jsx-a11y/accessible-emoji": 0,

    "react/jsx-filename-extension": 0,
    "react/prop-types": 0
  },
  "overrides": [
    {
      "files": ["**/*.test.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
